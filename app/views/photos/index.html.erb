<h4><%= link_to "Back" ,:back %></h4>
<h1>Photos index</h1>
<div id="photo-form" style="display:none;">

<%= link_to "image", new_photo_path %>
<% @photos.each do |photo| %>
	<%= link_to image_tag(photo.image.url), photo.image.url %>
	<%= link_to 'Remove', photo_path(photo), method: :delete, data: {confirm: 'Are you sure?'} %>
	<h4><%= photo.title %></h4>
<% end %>
</div>